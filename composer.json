    "license": "MIT",
    "authors": [
        {
            "name": "ArunBalaji06",
            "email": "arunbalaji@sparkouttech.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "laravel/framework": "^8.0",
        "illuminate/support": "^5.8|^6.0|^7.0|^8.0"
    },
    "require-dev": {
